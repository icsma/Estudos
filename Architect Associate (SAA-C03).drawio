        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-58" value="Amazon RDS" style="sketch=0;outlineConnect=0;strokeColor=#6c8ebf;fillColor=#dae8fc;dashed=0;verticalLabelPosition=middle;verticalAlign=bottom;align=center;html=1;whiteSpace=wrap;fontSize=10;fontStyle=1;spacing=3;shape=mxgraph.aws4.productIcon;prIcon=mxgraph.aws4.rds;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="838" y="1845.37" width="80" height="100" as="geometry" />
