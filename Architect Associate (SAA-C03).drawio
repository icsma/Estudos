        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-59" value="&lt;h1&gt;RDS&lt;/h1&gt;&lt;div&gt;&lt;b&gt;What:&lt;/b&gt;, autoscaling DB Service.&lt;br&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;Postgres |&amp;nbsp;&lt;/span&gt;MySQL | MariaDB | Oracle | MSSQL Server | Aurora&lt;/li&gt;&lt;/ul&gt;&lt;b&gt;Features:&lt;br&gt;&lt;/b&gt;Automated provisioning, OS patching&lt;br&gt;Monitoring dashboards&lt;br&gt;&lt;b&gt;&lt;br&gt;Performance:&lt;br&gt;&lt;/b&gt;Read replicas&lt;br&gt;Multi AZ for DR&lt;br&gt;&lt;br&gt;Maintenance windows for upgrades&lt;/div&gt;&lt;div&gt;Auto Scaling storage&lt;br&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;Auth:&amp;nbsp;&lt;/b&gt;&lt;br&gt;Username/Pass&lt;br&gt;IAM (mysql / postgres only)&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;span style=&quot;color: rgb(28 , 29 , 31) ; font-family: &amp;#34;sf pro text&amp;#34; , , &amp;#34;system-ui&amp;#34; , &amp;#34;roboto&amp;#34; , &amp;#34;segoe ui&amp;#34; , &amp;#34;helvetica&amp;#34; , &amp;#34;arial&amp;#34; , sans-serif , &amp;#34;apple color emoji&amp;#34; , &amp;#34;segoe ui emoji&amp;#34; , &amp;#34;segoe ui symbol&amp;#34; ; font-size: 16px ; background-color: rgb(247 , 249 , 250)&quot;&gt;A Multi-AZ standby cannot serve read requests.&amp;nbsp;&lt;/span&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="932.25" y="1845.37" width="270" height="394.63" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-60" value="&lt;h1&gt;RDS Backups&lt;/h1&gt;&lt;div&gt;&lt;span&gt;Automated Backups&lt;/span&gt;&lt;br&gt;&lt;ul&gt;&lt;li&gt;Daily full backup&lt;/li&gt;&lt;li&gt;Transaction logs every 5m&lt;/li&gt;&lt;li&gt;ability to restore in point from oldest backup to 5m ago&lt;/li&gt;&lt;li&gt;7 days retention&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;DB Snapshots&lt;/div&gt;&lt;/div&gt;&lt;div&gt;&lt;ul&gt;&lt;li&gt;Triggered by User&lt;/li&gt;&lt;li&gt;Eternal retention&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;" style="text;html=1;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1256.5" y="1855.87" width="190" height="240" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-61" value="&lt;h1&gt;&lt;span&gt;RDS Read Replicas&lt;/span&gt;&lt;/h1&gt;&lt;div&gt;&lt;ul&gt;&lt;li&gt;&lt;span&gt;Up to 5&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span&gt;Within AZ, Cross AZ or Cross Region&lt;/span&gt;&lt;/li&gt;&lt;li&gt;&lt;span&gt;Replication is Async (eventually consistent)&lt;/span&gt;&lt;/li&gt;&lt;li&gt;Can be promoted to own db&lt;/li&gt;&lt;li&gt;App updates connection string for reads&lt;/li&gt;&lt;li&gt;Charged for data transfer across region only.&lt;/li&gt;&lt;li&gt;Click modify, set to multi-az, automatically does it. No downtime.&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;&lt;b&gt;DR&lt;/b&gt;&lt;/div&gt;&lt;/div&gt;&lt;div&gt;&lt;ul&gt;&lt;li&gt;Sync replication&lt;/li&gt;&lt;li&gt;One dns name, auto-failover&lt;/li&gt;&lt;li&gt;no manual changes&lt;/li&gt;&lt;li&gt;Not for scaling&lt;/li&gt;&lt;li&gt;Multi-AZ rep is free&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;" style="text;html=1;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1259.88" y="2125.87" width="272.37" height="390" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-62" value="&lt;h1&gt;&lt;span&gt;RDS Encryption&lt;/span&gt;&lt;/h1&gt;&lt;div&gt;&lt;span&gt;&lt;b&gt;AT rest &lt;/b&gt;IF defined at launch&lt;/span&gt;&lt;br&gt;&lt;span&gt;If master is not, reads cannot be.&lt;/span&gt;&lt;br&gt;&lt;span&gt;&lt;b&gt;In flight:&lt;/b&gt; SSL&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;PostgreSQL: &lt;br&gt;&lt;/b&gt;rds.force_ssl=1 in the AWS RDS Console&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;MySQL:&lt;/b&gt;&lt;br&gt;GRANT USAGE ON *.* TO &#39;mysqluser&#39;@&#39;%&#39; REQUIRE SSL;&lt;br&gt;&lt;/div&gt;" style="text;html=1;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="932.25" y="2265.62" width="260" height="390" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-63" value="Amazon&#xa;Aurora" style="sketch=0;outlineConnect=0;fontColor=#232F3E;gradientColor=none;strokeColor=#ffffff;fillColor=#232F3E;dashed=0;verticalLabelPosition=middle;verticalAlign=bottom;align=center;html=1;whiteSpace=wrap;fontSize=10;fontStyle=1;spacing=3;shape=mxgraph.aws4.productIcon;prIcon=mxgraph.aws4.aurora;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1662.25" y="1750" width="80" height="110" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-64" value="&lt;h1&gt;Aurora&lt;/h1&gt;&lt;p&gt;&lt;b&gt;What:&lt;/b&gt; AWS FULLY Managed, optimised Postgres / MySQL. Scales automatically in increments of 10GB &amp;lt;=128GB.&lt;br&gt;&lt;span&gt;&lt;b&gt;&lt;br&gt;Why: &lt;/b&gt;5x performance over mysql, 3x over postgres on RDS&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span&gt;&lt;b&gt;Replicas:&lt;br&gt;&lt;/b&gt;Up to 15 (Mysql limited to 5)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;HA / Instant failover&lt;/p&gt;&lt;p&gt;20% more expensive but more efficient.&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1762.25" y="1740" width="230" height="290" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-65" value="&lt;h1&gt;Aurora HA / Read Scaling&lt;/h1&gt;&lt;p&gt;Shared storage volume&lt;br&gt;&lt;span&gt;6 copies over 3 AZ&lt;/span&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;&lt;ul&gt;&lt;li&gt;4 writes&lt;/li&gt;&lt;li&gt;3 for reads&lt;/li&gt;&lt;li&gt;Peer-to-peer replication, self healing&lt;/li&gt;&lt;li&gt;Striped stoarage across 100s of volumes&lt;/li&gt;&lt;/ul&gt;&lt;div&gt;Master is write&lt;/div&gt;&lt;div&gt;Master &amp;amp; &amp;lt;= 15 Reads for reads&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Automated failover &amp;lt; 30s&lt;br&gt;&lt;/div&gt;&lt;div&gt;Cross region replication&lt;/div&gt;&lt;div&gt;Point of time recovery&lt;/div&gt;&lt;div&gt;Auto patching, zero downtime&lt;/div&gt;&lt;div&gt;Security similar to RDS&lt;/div&gt;&lt;p&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1712.25" y="2050" width="290" height="346.38" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-66" value="&lt;h1&gt;Aurora custom Endpoints&lt;/h1&gt;&lt;p&gt;&lt;b&gt;What: &lt;/b&gt;Subset of instances, can be custom endpoint&lt;/p&gt;&lt;p&gt;&lt;b&gt;Why:&amp;nbsp;&lt;/b&gt;Run analytics on read replicas&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="2030.25" y="1750" width="312" height="100" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-67" value="&lt;h1&gt;Aurora Serverless&lt;/h1&gt;&lt;p&gt;&lt;b&gt;What:&lt;/b&gt; Serverless version of Aurora. &lt;br&gt;&lt;b&gt;&lt;br&gt;Why: &lt;/b&gt;Infrequent, intermittent or unpredictable workloads. &lt;br&gt;&lt;b&gt;&lt;br&gt;Pricing:&amp;nbsp;&lt;/b&gt;Pay per second, can be more cost effective.&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="2030.25" y="1860" width="312" height="142.38" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-68" value="&lt;h1&gt;Aurora Multi-Master&lt;/h1&gt;&lt;p&gt;Immediate failover for writes (HA)&lt;/p&gt;&lt;p&gt;Every node does R/W vs one master&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="2030.25" y="2027.62" width="302" height="140" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-69" value="&lt;h1&gt;Global Aurora&lt;/h1&gt;&lt;p&gt;&lt;b&gt;What: &lt;/b&gt;x-region read replicas - good for DR&lt;br&gt;&lt;span&gt;Aurora Global Database (recommended)&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;How: &lt;/b&gt;1 primary region for r/w, 5 secondary.&amp;nbsp;&lt;br&gt;&lt;span&gt;Up to 16 read replicas per secondary region.&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;div&gt;&lt;b&gt;Performance:&lt;/b&gt;&lt;span&gt; lag is &amp;lt;1s.&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;p&gt;&lt;span&gt;&lt;b&gt;DR&lt;/b&gt;: Promoting another region &amp;lt;1min&lt;/span&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="2030.25" y="2154.12" width="302" height="180.38" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-70" value="&lt;h1&gt;Aurora ML&lt;/h1&gt;&lt;div&gt;Amazon SageMaker (any ML model)&lt;/div&gt;&lt;div&gt;Amazon Comprehend (sentiment analysis)&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="2030.25" y="2346" width="262" height="94" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-71" value="Amazon ElastiCache" style="sketch=0;outlineConnect=0;fontColor=#232F3E;gradientColor=none;strokeColor=#ffffff;fillColor=#232F3E;dashed=0;verticalLabelPosition=middle;verticalAlign=bottom;align=center;html=1;whiteSpace=wrap;fontSize=10;fontStyle=1;spacing=3;shape=mxgraph.aws4.productIcon;prIcon=mxgraph.aws4.elasticache;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="921.88" y="3120" width="80" height="110" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-72" value="&lt;h1&gt;Elasticache&lt;/h1&gt;&lt;div&gt;&lt;b&gt;What: &lt;/b&gt;Caches are in memory databases, high perf, low latency.&lt;/div&gt;&lt;div&gt;&lt;b&gt;Why:&amp;nbsp;&lt;/b&gt;&amp;nbsp;read intensive workloads&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;b&gt;Notes:&lt;br&gt;&lt;/b&gt;Application can be stateless&lt;/div&gt;&lt;div&gt;Code changes required&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="1011.8800000000001" y="3120" width="420.37" height="242" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-73" value="&lt;h1&gt;EC for Redis&lt;/h1&gt;&lt;p&gt;&lt;b&gt;Availability: &lt;/b&gt;Multi AZ, auto failover&lt;/p&gt;&lt;p&gt;&lt;b&gt;Red replicas:&lt;br&gt;&lt;/b&gt;to scale reads, ++HA&lt;/p&gt;&lt;p&gt;&lt;b&gt;Data durabilitiy&lt;br&gt;&lt;/b&gt;Using AOF persistence&lt;/p&gt;&lt;p&gt;Backup &amp;amp; Restore&lt;br&gt;&lt;span&gt;Password/token&lt;br&gt;&lt;/span&gt;&lt;span&gt;SSL in flight&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Common exam use cases&lt;/b&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;REDIS SORTED SETS &lt;/b&gt;&lt;span&gt;guarantees uniqueness and ordering.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Session Management&lt;/b&gt;&lt;br&gt;Ask for session, no hit. Write it.&amp;nbsp;&lt;span&gt;&lt;br&gt;&lt;/span&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="1011.8800000000001" y="3291" width="200.37" height="340" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-74" value="&lt;h1&gt;EC for Memcached&lt;/h1&gt;&lt;p&gt;&lt;b&gt;Availability&lt;/b&gt;: No HA. &lt;br&gt;Multi node for partitioning (sharding)&lt;/p&gt;&lt;p&gt;&lt;span&gt;No persistence, backup or restore&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Notes:&lt;br&gt;&lt;/b&gt;Multi-threaded&lt;/p&gt;&lt;p&gt;&lt;b&gt;Auth:&lt;br&gt;&lt;/b&gt;SASL based auth (advanced)&lt;/p&gt;&lt;p&gt;&lt;b&gt;Common exam use cases&lt;/b&gt;&lt;/p&gt;&lt;div&gt;&lt;b&gt;Database query caching&lt;/b&gt;&lt;/div&gt;&lt;div&gt;Ask cache first, if not available go to RDS then store in EC.&amp;nbsp;&lt;/div&gt;&lt;div&gt;Requires invalidation strategy&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="1220.75" y="3295" width="241.13" height="340" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-75" value="" style="outlineConnect=0;dashed=0;verticalLabelPosition=bottom;verticalAlign=top;align=center;html=1;shape=mxgraph.aws3.route_53;fillColor=#F58536;gradientColor=none;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="-3255" y="-535.88" width="70.5" height="85.5" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-76" value="&lt;h1&gt;Route 53&lt;/h1&gt;&lt;p&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;Alias&lt;br&gt;&lt;/b&gt;root and non-root&lt;br&gt;Free + Native health check.&amp;nbsp;&lt;br&gt;Recognises IP changes in resource&lt;br&gt;A/AAAA for AWS resources&lt;br&gt;No TTL&lt;br&gt;&lt;br&gt;&lt;b&gt;Points to:&lt;br&gt;&lt;/b&gt;ELB, CF, API Gateway, EB, S3, VPCInterface Endpoints, GA Accelerator, R53 in same zone.&amp;nbsp;&lt;br&gt;&lt;br&gt;NOT for EC2 DNS Name.&amp;nbsp;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="-3172.5" y="-536.38" width="240" height="256.38" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-77" value="&lt;h1&gt;Routing policies&lt;/h1&gt;&lt;div&gt;&lt;b&gt;Simple&lt;/b&gt; - single value, no health.&lt;/div&gt;&lt;div&gt;&lt;b&gt;Weighted&lt;/b&gt; - shares of the request go to each. Can have health checks.&amp;nbsp;&lt;br&gt;&lt;b&gt;Failover&lt;/b&gt; - If 1 fails health check, fall back to 2.&lt;br&gt;&lt;b&gt;Latency&lt;/b&gt; - whichever is the closest in terms of speed to the server&lt;br&gt;&lt;b&gt;Geolocation&lt;/b&gt; - by country, continent etc.&lt;br&gt;&lt;b&gt;Geoproximity&lt;/b&gt; - geo distance to server. Can have dynamic bias&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="-3245.75" y="-230" width="392.5" height="140" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-78" value="&lt;h1&gt;&lt;b&gt;Health Checks&lt;/b&gt;&lt;br&gt;&lt;/h1&gt;&lt;div&gt;&lt;b&gt;Public endpoints only.&lt;/b&gt;&lt;br&gt;HTTP, HTTPS and TCP.&lt;br&gt;Default - If &amp;gt;18% say healthy&lt;/div&gt;&lt;div&gt;2xx/3xx status codes only.&lt;/div&gt;&lt;div&gt;Can read first 5120 bytes of response.&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;Requires&lt;/b&gt;: access through FW/SGs&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;For private / VPC:&amp;nbsp;&lt;br&gt;&lt;/b&gt;use Cloudwatch Metrics/Alarms then create a health check to check the alarm.&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;Calculated Health Checks&lt;/b&gt;&lt;/div&gt;&lt;div&gt;Combine up to 256 into one with AND, OR and NOT.&amp;nbsp;&lt;/div&gt;&lt;div&gt;Can configure pass rate&lt;/div&gt;&lt;div&gt;&lt;b&gt;&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;UseCase&lt;/b&gt;&lt;br&gt;Monitor how much of site is working, without forcing failover.&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="-2918.5" y="-537.63" width="270.75" height="353.5" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-79" value="AWS Elastic Beanstalk" style="sketch=0;outlineConnect=0;fontColor=#232F3E;gradientColor=none;strokeColor=#ffffff;fillColor=#232F3E;dashed=0;verticalLabelPosition=middle;verticalAlign=bottom;align=center;html=1;whiteSpace=wrap;fontSize=10;fontStyle=1;spacing=3;shape=mxgraph.aws4.productIcon;prIcon=mxgraph.aws4.elastic_beanstalk;labelBackgroundColor=none;" parent="1" vertex="1">
          <mxGeometry x="-1049.25" y="423" width="80" height="110" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-80" value="&lt;h1&gt;Elastic Beanstalk&lt;/h1&gt;&lt;p&gt;&lt;/p&gt;&lt;span&gt;&lt;b&gt;What: &lt;/b&gt;Centralised&lt;b&gt;&amp;nbsp;&lt;/b&gt;management of&amp;nbsp;Ec2, ASG, ELB, RDS etc.&lt;br&gt;&lt;/span&gt;&lt;span&gt;- Full control over config.&lt;/span&gt;&lt;br&gt;&lt;span&gt;- Free but pay for underlying instances&lt;/span&gt;&lt;br&gt;&lt;div&gt;&lt;b&gt;&lt;br&gt;&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;Common question:&lt;/b&gt;&lt;/div&gt;&lt;div&gt;Optimise launch of EB using Golden AMI and ec2 user-data&amp;nbsp;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;" style="text;html=1;strokeColor=none;fillColor=none;spacing=5;spacingTop=-20;whiteSpace=wrap;overflow=hidden;rounded=0;labelBackgroundColor=none;fontSize=12;" parent="1" vertex="1">
          <mxGeometry x="-943.75" y="419.5" width="263.75" height="180.5" as="geometry" />
        </mxCell>
        <mxCell id="vnq5Ngg2q1FD-P_rdCw7-81" value="Amazon S3" style="sketch=0;outlineConnect=0;fontColor=#232F3E;gradientColor=none;strokeColor=#ffffff;fillColor=#232F3E;dashed=0;verticalLabelPosition=middle;verticalAlign=bottom;align=center;html=1;whiteSpace=wrap;fontSize=10;fontStyle=1;spacing=3;shape=mxgraph.aws4.productIcon;prIcon=mxgraph.aws4.s3;labelBackgroundColor=none;" parent="1" vertex="1">
